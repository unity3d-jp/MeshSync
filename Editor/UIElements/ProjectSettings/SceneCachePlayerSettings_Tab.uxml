<UXML xmlns="UnityEngine.UIElements">
    
    <ScrollView name="SceneCachePlayerSettingsScrollView" show-vertical-scroller="true">

        <VisualElement name="HeaderContainer" class="general-header-container">
        </VisualElement>

        <VisualElement name="Content" class="project-settings-content">

            <VisualElement name="FieldContainer" class="field-container">
                <Label text="Generated Scene Cache Resources Path"/>
                <TextField class="project-settings-path-field" name="GeneratedSCResPathText"/>
                <Button text="..." name="OutputPathSelectButton"/>
            </VisualElement>
            
        </VisualElement>

        <VisualElement name="FooterContainer" class="general-footer-container">
            <Button name="ResetButton" text="Reset" class="footer-button"/>
        </VisualElement>
        
                
    </ScrollView>

        
</UXML>
